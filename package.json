{
  "name": "imgtonum",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://ryan-millard.github.io/Img2Num",
  "scripts": {
    "help": "node scripts/help.js",
    "docs": "npm run --prefix docs",
    "dev": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run docs start\"",
    "dev:debug": "npm run build-wasm:debug && vite",
    "dev:all:debug": "concurrently \"npm run dev:debug\" \"npm run docs start\"",
    "preview": "vite preview",
    "build": "npm run build-wasm && vite build",
    "build-js": "vite build",
    "build-wasm": "make -C src/wasm build",
    "build-wasm:debug": "make -C src/wasm debug",
    "clean": "npm run clean-wasm && npm run clean-js",
    "clean-js": "rm -rf dist",
    "clean-wasm": "make -C src/wasm clean",
    "format": "npm run format-js && npm run format-wasm",
    "format-js": "prettier --write .",
    "format-wasm": "node scripts/format-wasm.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "editorconfig-checker"
  },
  "dependencies": {
    "imagetracerjs": "^1.2.6",
    "lucide-react": "^0.555.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@tanstack/react-query": "^5.90.11",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "clang-format": "^1.8.0",
    "concurrently": "^9.2.1",
    "editorconfig-checker": "^6.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.20",
    "fast-glob": "^3.3.3",
    "fuzzy": "^0.1.3",
    "globals": "^16.5.0",
    "prettier": "^3.7.3",
    "vite": "^7.2.4",
    "vite-imagetools": "^9.0.2",
    "vite-plugin-sitemap": "^0.8.2"
  }
}
