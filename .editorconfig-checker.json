    ".git",
    "package-lock.json",
    "src/data/contributor-credits.json",
    "docs/node_modules",
    "docs/build",
    "docs/.docusaurus",
    "docs/package-lock.json",
    "src/wasm/build",
    "src/wasm/modules/image/build",
